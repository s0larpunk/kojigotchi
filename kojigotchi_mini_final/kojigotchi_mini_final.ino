// --- Setup ---
void setup() {
  Serial.begin(115200);

  pinMode(PIN_BUTTON_A, INPUT_PULLUP);
  pinMode(PIN_BUTTON_B, INPUT_PULLUP);
  pinMode(PIN_BUTTON_C, INPUT_PULLUP);

  initializeOLED();
  initializeAHT21();

  // Initial display setup
  showStatusFace(); // Always start with the Koji face
  lastUserInteractionTime = millis(); // Initialize interaction timer
}

// --- Main Loop ---
void loop() {
  unsigned long currentTime = millis();

  // 1. Sensor Data Acquisition (every SENSOR_READ_INTERVAL_MS)
  if (currentTime - lastSensorReadTime >= SENSOR_READ_INTERVAL_MS) {
    readSensorData();
    lastSensorReadTime = currentTime;
    newSensorDataAvailable = true; // Flag for screen refresh if in graph mode
  }

  // 2. Inactivity Timeout (revert to Koji face after INACTIVITY_TIMEOUT_MS)
  if (currentTime - lastUserInteractionTime > INACTIVITY_TIMEOUT_MS) {
    if (currentDisplayMode != MODE_STATUS || currentStatusBarSubMode != STATUS_NONE) {
      currentDisplayMode = MODE_STATUS;
      currentStatusBarSubMode = STATUS_NONE; // Revert to Koji face
      showStatusFace(); // Force refresh to the default face
    }
  }

  // 3. Display Updates
  if (currentDisplayMode == MODE_STATUS) {
    static unsigned long lastStatusDisplayTime = 0;
    if (currentTime - lastStatusDisplayTime >= STATUS_DISPLAY_REFRESH_MS) {
      if (currentStatusBarSubMode == STATUS_NONE) {
        showStatusFace();
      } else {
        showStatusBar(currentStatusBarSubMode);
      }
      lastStatusDisplayTime = currentTime;
    }
  } else if (currentDisplayMode == MODE_GRAPH) { // In graph mode
    if (newSensorDataAvailable) { // Only update graph if new data arrived
      int maxWindow = (getHistorySize(currentGraphSensor) - 1) / 256;

      if (autoFollowGraph && graphWindowIndex != maxWindow) {
        graphWindowIndex = maxWindow;
        windowSwitch = true; // Indicate a jump to a new window for plotting
      }
      plotGraph(currentGraphSensor);
      newSensorDataAvailable = false;
    }
  }

  // 4. Button Handling
  handleButtonPresses();
}

// --- Function Implementations ---

void initializeOLED() {
  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;) delay(10); // Infinite loop if display fails
  }
  display.display(); // Show initial Adafruit logo
  delay(2000);
  display.clearDisplay();
}

void initializeAHT21() {
  if (!aht.begin()) {
    Serial.println(F("AHT21 init failed!"));
    display.clearDisplay();
    display.setCursor(0, 30);
    display.setTextColor(SSD1306_WHITE);
    display.println(F("AHT21 failed"));
    display.display();
    for (;;) delay(10); // Infinite loop if AHT21 fails
  }
  Serial.println(F("AHT21 ready"));
}

void readSensorData() {
  sensors_event_t humidityEvent, tempEvent;
  aht.getEvent(&humidityEvent, &tempEvent);

  float temperature = tempEvent.temperature;
  float humidity = humidityEvent.relative_humidity;

  Serial.print(F("Temp (AHT21): "));
  Serial.print(temperature, 1);
  Serial.print(F(" C, Humidity (AHT21): "));
  Serial.print(humidity, 1);
  Serial.println(F(" %"));

  fullHistory_temp.push_back({millis(), temperature});
  fullHistory_humid.push_back({millis(), humidity});

  // Limit history size to prevent memory issues
  if (fullHistory_temp.size() > MAX_HISTORY_SIZE) {
    fullHistory_temp.erase(fullHistory_temp.begin());
  }
  if (fullHistory_humid.size() > MAX_HISTORY_SIZE) {
    fullHistory_humid.erase(fullHistory_humid.begin());
  }
}

void handleButtonPresses() {
  static unsigned long lastButtonPressTime = 0; // Static to retain value across calls

  // Debounce check
  if (millis() - lastButtonPressTime < BUTTON_DEBOUNCE_MS) {
    return;
  }

  bool buttonHandled = false;

  // --- Graph Mode Button Handling ---
  if (currentDisplayMode == MODE_GRAPH) {
    if (digitalRead(PIN_BUTTON_A) == LOW) {
      Serial.println(F("Button A pressed (Graph Mode: Scroll Left)"));
      if (graphWindowIndex > 0) {
        graphWindowIndex--;
        windowSwitch = true; // Indicate window change
      }
      autoFollowGraph = false; // Disable auto-follow when manually scrolling
      buttonHandled = true;
    } else if (digitalRead(PIN_BUTTON_B) == LOW) {
      Serial.println(F("Button B pressed (Graph Mode: Toggle Sensor)"));
      // Toggle sensor type
      currentGraphSensor = (currentGraphSensor == SENSOR_TEMP) ? SENSOR_HUMID : SENSOR_TEMP;
      // When switching sensor, jump to the end of the new sensor's data
      graphWindowIndex = (getHistorySize(currentGraphSensor) - 1) / 256;
      autoFollowGraph = true; // Re-enable auto-follow for the new sensor
      windowSwitch = true; // Indicate window change due to sensor switch
      buttonHandled = true;
    } else if (digitalRead(PIN_BUTTON_C) == LOW) {
      Serial.println(F("Button C pressed (Graph Mode: Scroll Right / Auto-follow Toggle)"));
      int maxWindow = (getHistorySize(currentGraphSensor) - 1) / 256;
      if (graphWindowIndex < maxWindow) {
        graphWindowIndex++;
        windowSwitch = true; // Indicate window change
        autoFollowGraph = false; // Disable auto-follow when manually scrolling
      } else {
        // If at the end, pressing C enables/disables auto-follow
        autoFollowGraph = !autoFollowGraph;
        if (autoFollowGraph) Serial.println(F("Auto-follow enabled."));
        else Serial.println(F("Auto-follow disabled."));
      }
      buttonHandled = true;
    }
  }
  // --- Status Mode Button Handling ---
  else { // This covers MODE_STATUS
    if (digitalRead(PIN_BUTTON_A) == LOW) {
      Serial.println(F("Button A pressed (Status Mode: Cycle Status Bar)"));
      if (currentStatusBarSubMode == STATUS_NONE) {
        currentStatusBarSubMode = STATUS_TEMP;
      } else if (currentStatusBarSubMode == STATUS_TEMP) {
        currentStatusBarSubMode = STATUS_HUMID;
      } else { // STATUS_HUMID
        currentStatusBarSubMode = STATUS_TEMP; // Cycle back to temp bar
      }
      buttonHandled = true;
    } else if (digitalRead(PIN_BUTTON_B) == LOW) {
      Serial.println(F("Button B pressed in status mode  switching to graph mode"));
      currentDisplayMode = MODE_GRAPH;
      autoFollowGraph = true;
      currentGraphSensor = SENSOR_TEMP; // Default to Temp graph
      graphWindowIndex = (getHistorySize(currentGraphSensor) - 1) / 256; // Jump to end of data
      windowSwitch = true; // Indicate a jump to a new window for the graph
      buttonHandled = true;
    } else if (digitalRead(PIN_BUTTON_C) == LOW) {
      Serial.println(F("Button C pressed (Status Mode: Cycle Status Bar)"));
      // This is similar to button A, providing an alternative way to cycle
      if (currentStatusBarSubMode == STATUS_NONE) {
        currentStatusBarSubMode = STATUS_HUMID;
      } else if (currentStatusBarSubMode == STATUS_HUMID) {
        currentStatusBarSubMode = STATUS_TEMP;
      } else { // STATUS_TEMP
        currentStatusBarSubMode = STATUS_HUMID; // Cycle back to humid bar
      }
      buttonHandled = true;
    }
  }

  if (buttonHandled) {
    lastButtonPressTime = millis();
    lastUserInteractionTime = millis(); // Reset inactivity timer on any button press

    // Force a display update after any button interaction
    if (currentDisplayMode == MODE_GRAPH) {
      plotGraph(currentGraphSensor);
    } else { // MODE_STATUS
      if (currentStatusBarSubMode == STATUS_NONE) {
        showStatusFace();
      } else {
        showStatusBar(currentStatusBarSubMode);
      }
    }
  }
}

void plotGraph(SensorType sensor) {
  std::vector<DataPoint>* history;
  const char* label;

  switch (sensor) {
    case SENSOR_TEMP:
      history = &fullHistory_temp;
      label = "Temp History, C";
      break;
    case SENSOR_HUMID:
      history = &fullHistory_humid;
      label = "Humidity History, %";
      break;
    default:
      return; // Should not happen with current enum usage
  }

  display.clearDisplay();
  // Draw the graph_ticks bitmap first. Ensure this bitmap has transparent (black) areas
  // where you want your text labels to appear.
  display.drawBitmap(0, 0, graph_ticks, 128, 64, SSD1306_WHITE);

  display.setCursor(8, 0); // Title at the top
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);
  display.println(label);

  int totalSamples = history->size();
  if (totalSamples < 2) { // Need at least two points to draw a line
    display.setCursor(0, 30);
    display.println(F("No data yet!"));
    display.display();
    return;
  }

  // --- Revised startIndex and endIndex logic for consistent windowing ---
  const int displayWindowSize = 256; // Fixed number of samples to display in one window

  int startIndex;
  int endIndex;

  // If autoFollowGraph is true, or if we are at the very last "page" of data,
  // we want to display the most recent `displayWindowSize` samples.
  // The (totalSamples - 1) / displayWindowSize calculation gives the index of the last full or partial window.
  int maxWindowIndex = (totalSamples > 0) ? (totalSamples - 1) / displayWindowSize : 0;


  if (autoFollowGraph || (graphWindowIndex >= maxWindowIndex)) {
    // Calculate the start index to show the latest `displayWindowSize` samples
    startIndex = totalSamples - displayWindowSize;
    if (startIndex < 0) startIndex = 0; // Ensure it doesn't go below 0

    endIndex = totalSamples; // Always go to the end of the available data

    // Update graphWindowIndex to reflect this "latest page" so manual scrolling starts from here
    graphWindowIndex = startIndex / displayWindowSize;
    if (startIndex % displayWindowSize != 0 && graphWindowIndex < maxWindowIndex) {
      // If startIndex is not a perfect multiple of displayWindowSize, and we're not already at maxWindowIndex,
      // increment graphWindowIndex to correctly represent the "page" it's on.
      graphWindowIndex++;
    }
     // A final check to ensure graphWindowIndex doesn't exceed maxWindowIndex when auto-following
    if (graphWindowIndex > maxWindowIndex) {
        graphWindowIndex = maxWindowIndex;
    }


  } else {
    // If autoFollowGraph is false (manual scroll)
    startIndex = graphWindowIndex * displayWindowSize;
    endIndex = startIndex + displayWindowSize;

    // Clamp endIndex to totalSamples if it overshoots
    if (endIndex > totalSamples) {
      endIndex = totalSamples;
      // If the window is now smaller than `displayWindowSize`, shift startIndex back
      if (endIndex - startIndex < displayWindowSize && startIndex > 0) {
        startIndex = endIndex - displayWindowSize;
        if (startIndex < 0) startIndex = 0; // Final check to ensure it's not negative
      }
    }
  }

  // Ensure we always have at least 2 samples to plot a line
  if (endIndex - startIndex < 2) {
    display.setCursor(0, 30);
    display.println(F("Not enough data to plot!"));
    display.display();
    return;
  }

  Serial.print(F("Plotting - totalSamples: ")); Serial.println(totalSamples);
  Serial.print(F("graphWindowIndex: ")); Serial.println(graphWindowIndex);
  Serial.print(F("startIndex: ")); Serial.println(startIndex);
  Serial.print(F("endIndex: ")); Serial.println(endIndex);

  // --- Calculate Y-axis min/max and labels ---
  float actualMin = 100.0, actualMax = 0.0; // Use float limits
  for (int i = startIndex; i < endIndex; ++i) {
    float val = (*history)[i].value;
    if (val < actualMin) actualMin = val;
    if (val > actualMax) actualMax = val;
  }

  // Add padding to min/max for better visualization, and ensure a range
  if (actualMin == actualMax) {
    actualMin -= 2.5; // Ensure at least a 5 unit range if values are constant
    actualMax += 2.5;
  } else {
    // Expand the range by a small percentage or fixed amount
    float range = actualMax - actualMin;
    actualMin -= range * 0.05; // 5% padding
    actualMax += range * 0.05;
  }

  // Round to nearest 5 or 10 units for nice graph scaling
  float yMin = floor(actualMin / 5.0) * 5.0;
  float yMax = ceil(actualMax / 5.0) * 5.0;

  // Ensure yMax is always greater than yMin for mapping
  if (yMax <= yMin) yMax = yMin + 5.0;


  // Y-axis labels (drawn on the left)
  display.setCursor(4, 11); display.print(yMax, 0); // 0 decimal places
  display.setCursor(4, 31); display.print((yMax + yMin) / 2, 0);
  display.setCursor(4, 51); display.print(yMin, 0);


  // --- X-axis labels (Time in minutes) ---
  // Each 'pixel column' of the graph represents 2 samples, so the whole 128px width represents 256 samples.
  // At SENSOR_READ_INTERVAL_MS (200ms) per sample, 256 samples is 51.2 seconds.
  // The labels should show elapsed time from the start of the *entire* history,
  // adjusted for the current window.

  unsigned long timeOfFirstSampleInHistory = 0;
  if (!history->empty()) {
    timeOfFirstSampleInHistory = (*history)[0].timestamp;
  }

  // Calculate the time of the *start* of the current displayed window relative to history start
  unsigned long windowStartTimestamp = 0;
  if (startIndex < totalSamples && totalSamples > 0) { // Ensure startIndex is valid
    windowStartTimestamp = (*history)[startIndex].timestamp;
  }

  // Define Y position for X-axis labels, just below the graph's bottom line
  const int X_LABEL_Y_POS = GRAPH_BOTTOM_Y + 2; // GRAPH_BOTTOM_Y is 52, so labels at Y=54

  // Total duration of one window in milliseconds
  const unsigned long WINDOW_DURATION_MS = displayWindowSize * SENSOR_READ_INTERVAL_MS;

  // Calculate timestamps for 4 points within the current window
  unsigned long labelTime1_ms = windowStartTimestamp;
  unsigned long labelTime2_ms = windowStartTimestamp + (WINDOW_DURATION_MS / 3);
  unsigned long label2_index = startIndex + (displayWindowSize / 3);
  if (label2_index >= endIndex && endIndex > 0) labelTime2_ms = (*history)[endIndex -1].timestamp; // if window is too small, clamp to end
  else if (label2_index < endIndex) labelTime2_ms = (*history)[label2_index].timestamp;


  unsigned long labelTime3_ms = windowStartTimestamp + (WINDOW_DURATION_MS * 2 / 3);
  unsigned long label3_index = startIndex + (displayWindowSize * 2 / 3);
  if (label3_index >= endIndex && endIndex > 0) labelTime3_ms = (*history)[endIndex -1].timestamp;
  else if (label3_index < endIndex) labelTime3_ms = (*history)[label3_index].timestamp;

  unsigned long labelTime4_ms = windowStartTimestamp + WINDOW_DURATION_MS; // Theoretical end of current window
  if (endIndex > 0) labelTime4_ms = (*history)[endIndex -1].timestamp; // Use actual end timestamp for last label


  display.setCursor(13, X_LABEL_Y_POS); display.print((labelTime1_ms - timeOfFirstSampleInHistory) / 60000UL); // Relative minutes
  display.setCursor(45, X_LABEL_Y_POS); display.print((labelTime2_ms - timeOfFirstSampleInHistory) / 60000UL);
  display.setCursor(76, X_LABEL_Y_POS); display.print((labelTime3_ms - timeOfFirstSampleInHistory) / 60000UL);
  display.setCursor(106, X_LABEL_Y_POS); display.print((labelTime4_ms - timeOfFirstSampleInHistory) / 60000UL); display.print("m");


  // --- Plot Data Points ---
  int lastX = -1; // Initialize to -1 to signify no previous point plotted
  int lastY = -1;

  // Iterate across screen pixels, each representing `samplesPerPixel` samples
  const int samplesPerPixel = 2; // 256 samples / 128 pixels = 2 samples/pixel

  for (int x_pixel = 0; x_pixel < SCREEN_WIDTH; ++x_pixel) {
    int currentSampleIndex = startIndex + (x_pixel * samplesPerPixel);
    if (currentSampleIndex >= endIndex) break; // Reached end of data in window

    // Take the average of `samplesPerPixel` samples for this pixel
    float sumValue = 0;
    int count = 0;
    for (int s = 0; s < samplesPerPixel; ++s) {
      if (currentSampleIndex + s < endIndex) {
        sumValue += (*history)[currentSampleIndex + s].value;
        count++;
      }
    }
    float avgValue = (count > 0) ? sumValue / count : 0;

    // Map to screen coordinates, ensuring the Y range is correct relative to borderY/topMargin
    int y_coord = map(avgValue, yMin, yMax, GRAPH_BOTTOM_Y, GRAPH_TOP_MARGIN);

    // Original 'windowSwitch' logic: skip drawing the first line after a window jump
    if (windowSwitch) {
      // If windowSwitch is true, this is the first point of the new window.
      // Just set lastX/lastY and reset the flag. Don't draw a line from the previous window.
      lastX = GRAPH_BORDER_X + x_pixel;
      lastY = y_coord;
      windowSwitch = false; // Reset the flag AFTER processing the first point
    } else if (lastX != -1) { // If not the first point, and a previous point exists
      display.drawLine(lastX, lastY, GRAPH_BORDER_X + x_pixel, y_coord, SSD1306_WHITE);
    }

    lastX = GRAPH_BORDER_X + x_pixel;
    lastY = y_coord;
  }
  display.display();
}

size_t getHistorySize(SensorType sensor) {
  switch (sensor) {
    case SENSOR_TEMP: return fullHistory_temp.size();
    case SENSOR_HUMID: return fullHistory_humid.size();
    default: return 0;
  }
}

void showStatusFace() {
  display.clearDisplay();

  sensors_event_t humidityEvent, tempEvent;
  aht.getEvent(&humidityEvent, &tempEvent);
  float temp = tempEvent.temperature;
  float humid = humidityEvent.relative_humidity;

  bool tempOk = (temp >= TEMP_KOJI_MIN && temp <= TEMP_KOJI_MAX);
  bool humidOk = (humid >= HUMID_KOJI_MIN && humid <= HUMID_KOJI_MAX);

  // 1. Base Koji face based on overall conditions
  if (tempOk && humidOk) {
    display.drawBitmap(0, 0, koji_v_happy, 128, 64, SSD1306_WHITE);
  } else if (!tempOk && !humidOk) {
    display.drawBitmap(0, 0, koji_v_sad, 128, 64, SSD1306_WHITE);
  } else { // One is good, one is bad
    display.drawBitmap(0, 0, koji_sad, 128, 64, SSD1306_WHITE);
  }

  // 2. Overlay temperature indicators
  if (temp < TEMP_KOJI_MIN) {
    display.drawBitmap(0, 0, temp_cold, 128, 64, SSD1306_WHITE);
  } else if (temp > TEMP_KOJI_MAX) {
    display.drawBitmap(0, 0, temp_warm, 128, 64, SSD1306_WHITE);
  }

  // 3. Overlay humidity and rice indicators
  if (humid < HUMID_KOJI_MIN) {
    display.drawBitmap(0, 0, hum_low, 128, 64, SSD1306_WHITE);
    display.drawBitmap(0, 0, rice_dry, 128, 64, SSD1306_WHITE);
  } else if (humid > HUMID_KOJI_MAX) {
    display.drawBitmap(0, 0, hum_high, 128, 64, SSD1306_WHITE);
    display.drawBitmap(0, 0, rice_wet, 128, 64, SSD1306_WHITE);
  } else {
    display.drawBitmap(0, 0, rice_good, 128, 64, SSD1306_WHITE);
  }

  display.display();
}

void showStatusBar(StatusBarSubMode mode) {
  display.clearDisplay();

  sensors_event_t humidityEvent, tempEvent;
  aht.getEvent(&humidityEvent, &tempEvent);
  float value = 0.0;
  const char* label = "";

  // Common bar parameters
  const int BAR_Y = 20;
  const int BAR_HEIGHT = 14;
  const int BAR_START_X = 38;
  const int BAR_END_X = 91; // Width of the fillable bar is 91 - 38 + 1 = 54 pixels

  if (mode == STATUS_TEMP) {
    display.drawBitmap(0, 0, status_bar_temp, 128, 64, SSD1306_WHITE);
    value = tempEvent.temperature;
    label = "Temp: ";

    // Clamp and map temperature: 25C to 35C
    int barFillX = map(constrain(value, 25.0, 35.0), 25, 35, BAR_START_X, BAR_END_X);

    // Draw vertical fill
    for (int x = BAR_START_X; x <= barFillX; x++) {
      display.drawLine(x, BAR_Y, x, BAR_Y + BAR_HEIGHT - 1, SSD1306_WHITE);
    }
    display.setCursor(0, 0);
    display.print(label);
    display.print(value, 1);
    display.print(F(" C"));

  } else if (mode == STATUS_HUMID) {
    display.drawBitmap(0, 0, status_bar_humid, 128, 64, SSD1306_WHITE);
    value = humidityEvent.relative_humidity;
    label = "Humid: ";

    // Clamp and map humidity: 70% to 100%
    int barFillX = map(constrain(value, 70.0, 100.0), 70, 100, BAR_START_X, BAR_END_X);

    for (int x = BAR_START_X; x <= barFillX; x++) {
      display.drawLine(x, BAR_Y, x, BAR_Y + BAR_HEIGHT - 1, SSD1306_WHITE);
    }
    display.setCursor(0, 0);
    display.print(label);
    display.print(value, 1);
    display.print(F(" %"));
  }
  display.display();
}