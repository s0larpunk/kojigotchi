const int MOISTURE_RAW_TOO_WET_THRESHOLD = 800;   // If raw reading is below this, it's too wet
const int MOISTURE_RAW_OPTIMAL_MIN = 1000;      // Lower bound of optimal raw moisture
const int MOISTURE_RAW_OPTIMAL_MAX = 2500;      // Upper bound of optimal raw moisture
const int MOISTURE_RAW_TOO_DRY_THRESHOLD = 3000;  // If raw reading is above this, it's too dry

const int CO2_HIGH_THRESHOLD = 1000; // Example
const int CO2_V_HIGH_THRESHOLD = 2000; // Example
const int CO2_MAX_GRAPH = 5000; // Max value for CO2 graph scaling

const int VOC_LOW_THRESHOLD = 50; // Example
const int VOC_MED_THRESHOLD = 200; // Example
const int VOC_HIGH_THRESHOLD = 500; // Example
const int VOC_MAX_GRAPH = 1000; // Max value for VOC graph scaling

// Optional: Limit history size to prevent memory issues on long runs
const size_t MAX_HISTORY_SIZE = 3000; // e.g., 10 minutes of data at 0.2s intervals (3000 samples)

// --- Data Structures ---
struct DataPoint {
  unsigned long timestamp;
  float value;
};

std::vector<DataPoint> fullHistory_temp; // AHT21 Temperature
std::vector<DataPoint> fullHistory_humid; // AHT21 Humidity
std::vector<DataPoint> fullHistory_probe_temp; // DS18B20 Temperature
std::vector<DataPoint> fullHistory_moisture; // HD-38 Moisture
std::vector<DataPoint> fullHistory_co2; // ENS160 CO2
std::vector<DataPoint> fullHistory_voc; // ENS160 VOC

// --- Enums ---
enum SensorType {
  SENSOR_TEMP, // AHT21
  SENSOR_HUMID, // AHT21
  SENSOR_PROBE_TEMP, // DS18B20
  SENSOR_MOISTURE, // HD-38
  SENSOR_CO2, // ENS160
  SENSOR_VOC // ENS160
};

enum DisplayMode {
  MODE_STATUS,        // Represents the Koji face or status bar
  MODE_GRAPH          // Represents either Temp or Humid graph
};

enum StatusBarSubMode { // Used when DisplayMode is MODE_STATUS
  STATUS_NONE,        // Koji face
  STATUS_TEMP,        // AHT21 Temp
  STATUS_HUMID,       // AHT21 Humid
  STATUS_PROBE_TEMP,  // DS18B20 Temp
  STATUS_MOISTURE,    // HD-38 Moisture
  STATUS_CO2,         // ENS160 CO2
  STATUS_VOC          // ENS160 VOC
};

// --- Global Variables ---
DisplayMode currentDisplayMode = MODE_STATUS;
StatusBarSubMode currentStatusBarSubMode = STATUS_NONE; // Tracks which status bar or face is shown

unsigned long lastSensorReadTime = 0;
unsigned long lastUserInteractionTime = 0;
bool newSensorDataAvailable = false; // Flag for screen refresh in graph mode

int graphWindowIndex = 0; // Which 'window' of 256 samples is being displayed for graph
bool autoFollowGraph = true; // Automatically follow the latest data in graph mode
bool windowSwitch = false; // Flag to indicate a window change (for graph plotting's first line)

SensorType currentGraphSensor = SENSOR_TEMP; // Which sensor's graph is currently displayed

// --- Function Prototypes ---
void initializeOLED();
void initializeAHT21();
void initializeENS160();
void initializeDS18B20();
void readSensorData();
void handleButtonPresses();
void plotGraph(SensorType sensor);
size_t getHistorySize(SensorType sensor);
void showStatusFace();
void showStatusBar(StatusBarSubMode mode);
long mapFloat(float x, float in_min, float in_max, long out_min, long out_max); // Declare mapFloat

// --- Setup ---
void setup() {
  Serial.begin(115200);
  delay(100);

  // Initialize I2C for ESP32
  Wire.begin(I2C_SDA, I2C_SCL);
  yield(); // Yield after Wire.begin

  // Configure button pins as inputs with internal pull-up resistors
  pinMode(BUTTON_A_PIN, INPUT_PULLUP);
  pinMode(BUTTON_B_PIN, INPUT_PULLUP);
  pinMode(BUTTON_C_PIN, INPUT_PULLUP);
  yield(); // Yield after pinMode

  initializeOLED();
  yield(); // Yield after OLED init
  initializeAHT21();
  yield(); // Yield after AHT21 init
  initializeENS160();
  yield(); // Yield after ENS160 init
  initializeDS18B20();
  yield(); // Yield after DS18B20 init

  // Initial display setup
  showStatusFace(); // Always start with the Koji face
  lastUserInteractionTime = millis(); // Initialize interaction timer
  yield(); // Yield after initial display
}

// --- Main Loop ---
void loop() {
  unsigned long currentTime = millis();
  yield(); // Initial yield in loop

  // 1. Sensor Data Acquisition (every SENSOR_READ_INTERVAL_MS)
  if (currentTime - lastSensorReadTime >= SENSOR_READ_INTERVAL_MS) {
    readSensorData();
    lastSensorReadTime = currentTime;
    newSensorDataAvailable = true; // Flag for screen refresh if in graph mode
    yield(); // Yield after sensor read
  }

  // 2. Inactivity Timeout (revert to Koji face after INACTIVITY_TIMEOUT_MS)
  if (currentTime - lastUserInteractionTime > INACTIVITY_TIMEOUT_MS) {
    if (currentDisplayMode != MODE_STATUS || currentStatusBarSubMode != STATUS_NONE) {
      currentDisplayMode = MODE_STATUS;
      currentStatusBarSubMode = STATUS_NONE; // Revert to Koji face
      showStatusFace(); // Force refresh to the default face
      yield(); // Yield after showing status face
    }
  }

  // 3. Display Updates
  if (currentDisplayMode == MODE_STATUS) {
    static unsigned long lastStatusDisplayTime = 0;
    if (currentTime - lastStatusDisplayTime >= STATUS_DISPLAY_REFRESH_MS) {
      if (currentStatusBarSubMode == STATUS_NONE) {
        showStatusFace();
      } else {
        showStatusBar(currentStatusBarSubMode);
      }
      lastStatusDisplayTime = currentTime;
      yield(); // Yield after status bar/face update
    }
  } else if (currentDisplayMode == MODE_GRAPH) { // In graph mode
    if (newSensorDataAvailable) { // Only update graph if new data arrived
      int maxWindow = (getHistorySize(currentGraphSensor) - 1) / SCREEN_WIDTH; // Using SCREEN_WIDTH as graph window size
      if (autoFollowGraph && graphWindowIndex != maxWindow) {
        graphWindowIndex = maxWindow;
        windowSwitch = true; // Indicate a jump to a new window for plotting
      }
      plotGraph(currentGraphSensor);
      newSensorDataAvailable = false;
      yield(); // Yield after plotting graph
    }
  }

  // 4. Button Handling
  handleButtonPresses();
  yield(); // Final yield in loop
}

// --- Function Implementations ---

void initializeOLED() {
  if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;) yield(); // Infinite loop if display fails, yield to prevent watchdog
  }
  yield(); // Yield before clearDisplay
  display.clearDisplay();
  display.display(); // Show initial Adafruit logo
  delay(2000);
  yield(); // Yield after delay
  display.clearDisplay();
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(0, 0);
  display.println("OLED ready");
  display.display();
  yield(); // Yield after OLED display
}

void initializeAHT21() {
  if (!aht.begin()) {
    Serial.println(F("AHT21 init failed!"));
    yield(); // Yield before clearDisplay
    display.clearDisplay();
    display.setCursor(0, 30);
    display.setTextColor(SSD1306_WHITE);
    display.println(F("AHT21 failed"));
    display.display();
    for (;;) yield(); // Infinite loop if AHT21 fails, yield to prevent watchdog
  }
  Serial.println(F("AHT21 ready"));
  yield(); // Yield after AHT21 init
}

void initializeENS160() {
  if (airQualitySensor.begin() != 0) {
    Serial.println("ENS160 init failed!");
    yield(); // Yield before clearDisplay
    display.clearDisplay();
    display.setCursor(0, 20);
    display.println("ENS160 failed");
    display.display();
    while (1) yield(); // Infinite loop, yield to prevent watchdog
  }
  Serial.println("ENS160 ready");
  yield(); // Yield after ENS160 init
}

void initializeDS18B20() {
  tempSensorDS18B20.begin();
  Serial.print("Found ");
  Serial.print(tempSensorDS18B20.getDeviceCount(), DEC);
  Serial.println(" temperature devices.");
  yield(); // Yield after DS18B20 init
}

void readSensorData() {
  unsigned long currentMillis = millis(); // Use currentMillis for all timestamps in this read cycle

  // Read AHT21 data
  sensors_event_t humidityEvent, tempEvent;
  aht.getEvent(&humidityEvent, &tempEvent);
  float temperature_aht21 = tempEvent.temperature;
  float humidity_aht21 = humidityEvent.relative_humidity;
  fullHistory_temp.push_back({currentMillis, temperature_aht21});
  fullHistory_humid.push_back({currentMillis, humidity_aht21});
  yield(); // Yield after AHT reads

  // Read ENS160 data
  uint16_t tvoc = airQualitySensor.getTVOC();
  uint16_t eco2 = airQualitySensor.getECO2();
  fullHistory_co2.push_back({currentMillis, (float)eco2});
  fullHistory_voc.push_back({currentMillis, (float)tvoc});
  yield(); // Yield after ENS160 reads

  // Read DS18B20 data
  tempSensorDS18B20.requestTemperatures();
  float temperature_ds18b20 = tempSensorDS18B20.getTempCByIndex(0);
  fullHistory_probe_temp.push_back({currentMillis, temperature_ds18b20});
  yield(); // Yield after DS18B20 read

  // Read HD-38 moisture data (reduced samples for faster operation)
  float rawSoilMoisture = 0;
  for (int i = 0; i < 10; i++) { // Averaging 10 readings for stability
    rawSoilMoisture += analogRead(MOISTURE_PIN);
    delay(1);
    yield(); // Yield within the loop
  }
  rawSoilMoisture /= 10.0; // Divide by 10 as we took 10 samples
  fullHistory_moisture.push_back({currentMillis, rawSoilMoisture});
  yield(); // Yield after moisture read

  // Limit history size to prevent memory issues for all vectors
  if (fullHistory_temp.size() > MAX_HISTORY_SIZE) fullHistory_temp.erase(fullHistory_temp.begin());
  if (fullHistory_humid.size() > MAX_HISTORY_SIZE) fullHistory_humid.erase(fullHistory_humid.begin());
  if (fullHistory_probe_temp.size() > MAX_HISTORY_SIZE) fullHistory_probe_temp.erase(fullHistory_probe_temp.begin());
  if (fullHistory_moisture.size() > MAX_HISTORY_SIZE) fullHistory_moisture.erase(fullHistory_moisture.begin());
  if (fullHistory_co2.size() > MAX_HISTORY_SIZE) fullHistory_co2.erase(fullHistory_co2.begin());
  if (fullHistory_voc.size() > MAX_HISTORY_SIZE) fullHistory_voc.erase(fullHistory_voc.begin());
  yield(); // Yield after history management

  Serial.print(F("Temp (AHT21): ")); Serial.print(temperature_aht21, 1); Serial.print(F(" C, "));
  Serial.print(F("Hum (AHT21): ")); Serial.print(humidity_aht21, 1); Serial.print(F(" %, "));
  Serial.print(F("Temp (DS18B20): ")); Serial.print(temperature_ds18b20, 1); Serial.print(F(" C, "));
  Serial.print(F("Moisture: ")); Serial.print(rawSoilMoisture, 0); Serial.print(F(", "));
  Serial.print(F("eCO2: ")); Serial.print(eco2); Serial.print(F(" ppm, "));
  Serial.print(F("TVOC: ")); Serial.print(tvoc); Serial.println(F(" ppb"));
  yield(); // Yield after serial print
}

void handleButtonPresses() {
  static unsigned long lastButtonPressTime = 0;

  // Debounce check
  if (millis() - lastButtonPressTime < BUTTON_DEBOUNCE_MS) {
    return;
  }

  bool buttonHandled = false;

  // --- Graph Mode Button Handling ---
  if (currentDisplayMode == MODE_GRAPH) {
    if (digitalRead(BUTTON_A_PIN) == LOW) {
      Serial.println(F("Button A pressed (Graph Mode: Scroll Left)"));
      if (graphWindowIndex > 0) {
        graphWindowIndex--;
        windowSwitch = true;
      }
      autoFollowGraph = false;
      buttonHandled = true;
    } else if (digitalRead(BUTTON_B_PIN) == LOW) {
      Serial.println(F("Button B pressed (Graph Mode: Toggle Sensor)"));
      // Toggle sensor type
      switch (currentGraphSensor) {
        case SENSOR_TEMP:
          currentGraphSensor = SENSOR_HUMID;
          break;
        case SENSOR_HUMID:
          currentGraphSensor = SENSOR_PROBE_TEMP;
          break;
        case SENSOR_PROBE_TEMP:
          currentGraphSensor = SENSOR_MOISTURE;
          break;
        case SENSOR_MOISTURE:
          currentGraphSensor = SENSOR_CO2;
          break;
        case SENSOR_CO2:
          currentGraphSensor = SENSOR_VOC;
          break;
        case SENSOR_VOC:
          currentGraphSensor = SENSOR_TEMP; // Cycle back to AHT21 Temp
          break;
      }
      // When changing sensor, reset window to auto-follow new data
      graphWindowIndex = (getHistorySize(currentGraphSensor) - 1) / SCREEN_WIDTH;
      autoFollowGraph = true;
      windowSwitch = true;
      buttonHandled = true;
    } else if (digitalRead(BUTTON_C_PIN) == LOW) {
      Serial.println(F("Button C pressed (Graph Mode: Scroll Right / Auto-follow Toggle)"));
      int maxWindow = (getHistorySize(currentGraphSensor) - 1) / SCREEN_WIDTH;
      if (graphWindowIndex < maxWindow) {
        graphWindowIndex++;
        windowSwitch = true; // Indicate window change
        autoFollowGraph = false;
      } else {
        // If at the end, pressing C enables/disables auto-follow
        autoFollowGraph = !autoFollowGraph;
        if (autoFollowGraph) Serial.println(F("Auto-follow enabled."));
        else Serial.println(F("Auto-follow disabled."));
      }
      buttonHandled = true;
    }
  }
  // --- Status Mode Button Handling ---
  else { // This covers MODE_STATUS
    if (digitalRead(BUTTON_A_PIN) == LOW) {
      Serial.println(F("Button A pressed (Status Mode: Cycle Status Bar)"));
      switch (currentStatusBarSubMode) {
        case STATUS_NONE:
          currentStatusBarSubMode = STATUS_TEMP;
          break;
        case STATUS_TEMP:
          currentStatusBarSubMode = STATUS_HUMID;
          break;
        case STATUS_HUMID:
          currentStatusBarSubMode = STATUS_PROBE_TEMP;
          break;
        case STATUS_PROBE_TEMP:
          currentStatusBarSubMode = STATUS_MOISTURE;
          break;
        case STATUS_MOISTURE:
          currentStatusBarSubMode = STATUS_CO2;
          break;
        case STATUS_CO2:
          currentStatusBarSubMode = STATUS_VOC;
          break;
        case STATUS_VOC:
          currentStatusBarSubMode = STATUS_TEMP; // Cycle back to AHT21 Temp
          break;
      }
      buttonHandled = true;
    } else if (digitalRead(BUTTON_B_PIN) == LOW) {
      Serial.println(F("Button B pressed in status mode — switching to graph mode"));
      currentDisplayMode = MODE_GRAPH;
      autoFollowGraph = true;
      currentGraphSensor = SENSOR_TEMP; // Default to AHT21 Temp graph
      graphWindowIndex = (getHistorySize(currentGraphSensor) - 1) / SCREEN_WIDTH;
      windowSwitch = true;
      buttonHandled = true;
    } else if (digitalRead(BUTTON_C_PIN) == LOW) {
      Serial.println(F("Button C pressed (Status Mode: Revert to Koji Face)"));
      currentStatusBarSubMode = STATUS_NONE; // Revert to Koji face
      buttonHandled = true;
    }
  }

  if (buttonHandled) {
    lastButtonPressTime = millis();
    lastUserInteractionTime = millis();

    // Force a display update after any button interaction
    if (currentDisplayMode == MODE_GRAPH) {
      plotGraph(currentGraphSensor);
    } else { // MODE_STATUS
      if (currentStatusBarSubMode == STATUS_NONE) {
        showStatusFace();
      } else {
        showStatusBar(currentStatusBarSubMode);
      }
    }
  }
  yield(); // Yield after button handling
}

void plotGraph(SensorType sensor) {
  std::vector<DataPoint>* history;
  const char* label;
  // float yMin_default, yMax_default; // No longer used, actual min/max are dynamic

  switch (sensor) {
    case SENSOR_TEMP:
      history = &fullHistory_temp;
      label = "AHT21 Temp, C";
      break;
    case SENSOR_HUMID:
      history = &fullHistory_humid;
      label = "AHT21 Humidity, %";
      break;
    case SENSOR_PROBE_TEMP:
      history = &fullHistory_probe_temp;
      label = "DS18B20 Temp, C";
      break;
    case SENSOR_MOISTURE:
      history = &fullHistory_moisture;
      label = "Moisture, Raw";
      break;
    case SENSOR_CO2:
      history = &fullHistory_co2;
      label = "eCO2, ppm";
      break;
    case SENSOR_VOC:
      history = &fullHistory_voc;
      label = "TVOC, ppb";
      break;
    default:
      return;
  }

  yield(); // Yield before clearDisplay
  display.clearDisplay();
  display.drawBitmap(0, 0, graph_ticks, 128, 64, SSD1306_WHITE);
  yield(); // Yield after bitmap draw

  display.setCursor(8, 0); // Title at the top
  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);
  display.println(label);

  int totalSamples = history->size();
  if (totalSamples < 2) {
    display.setCursor(0, 30);
    display.println(F("No data yet!"));
    display.display();
    return;
  }

  const int displayWindowSize = SCREEN_WIDTH; // Plotting for full screen width
  int startIndex;
  int endIndex;

  int maxWindowIndex = (totalSamples > 0) ? (totalSamples - 1) / displayWindowSize : 0;
  if (autoFollowGraph || (graphWindowIndex >= maxWindowIndex)) {
    startIndex = totalSamples - displayWindowSize;
    if (startIndex < 0) startIndex = 0;
    endIndex = totalSamples;
    graphWindowIndex = startIndex / displayWindowSize;
    if (startIndex % displayWindowSize != 0 && graphWindowIndex < maxWindowIndex) {
      graphWindowIndex++;
    }
    if (graphWindowIndex > maxWindowIndex) {
      graphWindowIndex = maxWindowIndex;
    }
  } else {
    startIndex = graphWindowIndex * displayWindowSize;
    endIndex = startIndex + displayWindowSize;
    if (endIndex > totalSamples) {
      endIndex = totalSamples;
      if (endIndex - startIndex < displayWindowSize && startIndex > 0) {
        startIndex = endIndex - displayWindowSize;
        if (startIndex < 0) startIndex = 0;
      }
    }
  }

  if (endIndex - startIndex < 2) {
    display.setCursor(0, 30);
    display.println(F("Not enough data to plot!"));
    display.display();
    return;
  }

  // Serial.print(F("Plotting - totalSamples: ")); Serial.println(totalSamples);
  // Serial.print(F("graphWindowIndex: ")); Serial.println(graphWindowIndex);
  // Serial.print(F("startIndex: ")); Serial.println(startIndex);
  // Serial.print(F("endIndex: ")); Serial.println(endIndex);

  float actualMin = history->at(startIndex).value, actualMax = history->at(startIndex).value; // Initialize with first value
  for (int i = startIndex; i < endIndex; ++i) {
    float val = (*history)[i].value;
    if (val < actualMin) actualMin = val;
    if (val > actualMax) actualMax = val;
    if (i % 10 == 0) yield(); // Yield periodically in this loop
  }

  if (actualMin == actualMax) {
    actualMin -= 2.5;
    actualMax += 2.5;
  } else {
    float range = actualMax - actualMin;
    actualMin -= range * 0.05;
    actualMax += range * 0.05;
  }

  float yMin = floor(actualMin / 5.0) * 5.0;
  float yMax = ceil(actualMax / 5.0) * 5.0;

  if (yMax <= yMin) yMax = yMin + 5.0;

  display.setCursor(4, 11); display.print(yMax, 0);
  display.setCursor(4, 31);
  display.print((yMax + yMin) / 2, 0);
  display.setCursor(4, 51); display.print(yMin, 0);
  yield(); // Yield after printing Y-axis labels

  unsigned long timeOfFirstSampleInHistory = 0;
  if (!history->empty()) {
    timeOfFirstSampleInHistory = (*history)[0].timestamp;
  }

  unsigned long windowStartTimestamp = 0;
  if (startIndex < totalSamples && totalSamples > 0) {
    windowStartTimestamp = (*history)[startIndex].timestamp;
  }

  const int X_LABEL_Y_POS = GRAPH_BOTTOM_Y + 2;

  const unsigned long WINDOW_DURATION_MS = displayWindowSize * SENSOR_READ_INTERVAL_MS;

  unsigned long labelTime1_ms = windowStartTimestamp;
  unsigned long labelTime2_ms = windowStartTimestamp + (WINDOW_DURATION_MS / 3);
  unsigned long label2_index = startIndex + (displayWindowSize / 3);
  if (label2_index >= endIndex && endIndex > 0) labelTime2_ms = (*history)[endIndex -1].timestamp;
  else if (label2_index < endIndex) labelTime2_ms = (*history)[label2_index].timestamp;

  unsigned long labelTime3_ms = windowStartTimestamp + (WINDOW_DURATION_MS * 2 / 3);
  unsigned long label3_index = startIndex + (displayWindowSize * 2 / 3);
  if (label3_index >= endIndex && endIndex > 0) labelTime3_ms = (*history)[endIndex -1].timestamp;
  else if (label3_index < endIndex) labelTime3_ms = (*history)[label3_index].timestamp;

  unsigned long labelTime4_ms = windowStartTimestamp + WINDOW_DURATION_MS;
  if (endIndex > 0) labelTime4_ms = (*history)[endIndex -1].timestamp;

  display.setCursor(13, X_LABEL_Y_POS); display.print((labelTime1_ms - timeOfFirstSampleInHistory) / 60000UL);
  display.setCursor(45, X_LABEL_Y_POS); display.print((labelTime2_ms - timeOfFirstSampleInHistory) / 60000UL);
  display.setCursor(76, X_LABEL_Y_POS); display.print((labelTime3_ms - timeOfFirstSampleInHistory) / 60000UL);
  display.setCursor(106, X_LABEL_Y_POS);
  display.print((labelTime4_ms - timeOfFirstSampleInHistory) / 60000UL); display.print("m");
  yield(); // Yield after printing X-axis labels

  int lastX = -1;
  int lastY = -1;
  const int samplesPerPixel = 1; // Assuming one sample per pixel column for a clear graph

  for (int x_pixel = 0; x_pixel < SCREEN_WIDTH; ++x_pixel) {
    int currentSampleIndex = startIndex + (x_pixel * samplesPerPixel);
    if (currentSampleIndex >= endIndex) break;

    float val = (*history)[currentSampleIndex].value;
    int y_coord = mapFloat(val, yMin, yMax, GRAPH_BOTTOM_Y, GRAPH_TOP_MARGIN);

    if (windowSwitch) {
      lastX = GRAPH_BORDER_X + x_pixel;
      lastY = y_coord;
      windowSwitch = false;
    } else if (lastX != -1) {
      display.drawLine(lastX, lastY, GRAPH_BORDER_X + x_pixel, y_coord, SSD1306_WHITE);
    }

    lastX = GRAPH_BORDER_X + x_pixel;
    lastY = y_coord;
    if (x_pixel % 10 == 0) yield(); // Yield periodically during line drawing
  }
  display.display();
  yield(); // Final yield after display update
}

size_t getHistorySize(SensorType sensor) {
  switch (sensor) {
    case SENSOR_TEMP: return fullHistory_temp.size();
    case SENSOR_HUMID: return fullHistory_humid.size();
    case SENSOR_PROBE_TEMP: return fullHistory_probe_temp.size();
    case SENSOR_MOISTURE: return fullHistory_moisture.size();
    case SENSOR_CO2: return fullHistory_co2.size();
    case SENSOR_VOC: return fullHistory_voc.size();
    default: return 0;
  }
}

void showStatusFace() {
  yield(); // Yield before clearDisplay
  display.clearDisplay();
  yield(); // Yield after clear

  sensors_event_t humidityEvent, tempEventAHT;
  aht.getEvent(&humidityEvent, &tempEventAHT);
  float tempAHT = tempEventAHT.temperature;
  float humidAHT = humidityEvent.relative_humidity;
  yield(); // Yield after AHT read

  tempSensorDS18B20.requestTemperatures();
  float tempDS18B20 = tempSensorDS18B20.getTempCByIndex(0);
  yield(); // Yield after DS18B20 read

  float rawSoilMoisture = analogRead(MOISTURE_PIN); // Reading once for status display
  yield(); // Yield after moisture read

  uint16_t tvoc = airQualitySensor.getTVOC();
  uint16_t eco2 = airQualitySensor.getECO2();
  yield(); // Yield after ENS160 read

  bool tempOkAHT = (tempAHT >= TEMP_KOJI_MIN && tempAHT <= TEMP_KOJI_MAX);
  bool humidOkAHT = (humidAHT >= HUMID_KOJI_MIN && humidAHT <= HUMID_KOJI_MAX);
  bool tempOkDS18B20 = (tempDS18B20 >= TEMP_KOJI_MIN && tempDS18B20 <= TEMP_KOJI_MAX);

  // Corrected moisture logic based on optimal/min/max
  // bool moistureGood = (rawSoilMoisture > MOISTURE_MIN && rawSoilMoisture < MOISTURE_MAX && abs(rawSoilMoisture - MOISTURE_OPTIMAL) <= 200); // within 200 of optimal
  // bool moistureDry = (rawSoilMoisture <= MOISTURE_MIN);
  // bool moistureWet = (rawSoilMoisture >= MOISTURE_MAX);

  // NEW moisture logic based on 0=wet, 4095=dry
  bool moistureGood = (rawSoilMoisture >= MOISTURE_RAW_OPTIMAL_MIN && rawSoilMoisture <= MOISTURE_RAW_OPTIMAL_MAX);
  bool moistureDry = (rawSoilMoisture > MOISTURE_RAW_TOO_DRY_THRESHOLD);
  bool moistureWet = (rawSoilMoisture < MOISTURE_RAW_TOO_WET_THRESHOLD);


  // 1. Base Koji face based on overall conditions (can be made more complex)
  if (tempOkAHT && humidOkAHT && tempOkDS18B20 && moistureGood && eco2 < CO2_HIGH_THRESHOLD && tvoc < VOC_MED_THRESHOLD) {
    display.drawBitmap(0, 0, koji_v_happy, 128, 64, SSD1306_WHITE);
  } else if (!tempOkAHT && !humidOkAHT && !tempOkDS18B20 && (moistureDry || moistureWet) && (eco2 >= CO2_HIGH_THRESHOLD || tvoc >= VOC_MED_THRESHOLD)) {
    display.drawBitmap(0, 0, koji_v_sad, 128, 64, SSD1306_WHITE);
  } else {
    display.drawBitmap(0, 0, koji_sad, 128, 64, SSD1306_WHITE);
  }
  yield(); // Yield after base face

  // 2. Overlay temperature indicators (AHT21 and DS18B20)
  if (tempAHT < TEMP_KOJI_MIN || tempDS18B20 < TEMP_KOJI_MIN) {
    display.drawBitmap(0, 0, temp_cold, 128, 64, SSD1306_WHITE);
  } else if (tempAHT > TEMP_KOJI_MAX || tempDS18B20 > TEMP_KOJI_MAX) {
    display.drawBitmap(0, 0, temp_warm, 128, 64, SSD1306_WHITE);
  }
  yield(); // Yield after temp overlays

  // 3. Overlay humidity and rice indicators (AHT21 and Moisture)
  if (humidAHT < HUMID_KOJI_MIN || moistureDry) {
    display.drawBitmap(0, 0, hum_low, 128, 64, SSD1306_WHITE);
    display.drawBitmap(0, 0, rice_dry, 128, 64, SSD1306_WHITE);
  } else if (humidAHT > HUMID_KOJI_MAX || moistureWet) {
    display.drawBitmap(0, 0, hum_high, 128, 64, SSD1306_WHITE);
    display.drawBitmap(0, 0, rice_wet, 128, 64, SSD1306_WHITE);
  } else if (moistureGood) { // Only draw rice_good if moisture is actually good
    display.drawBitmap(0, 0, rice_good, 128, 64, SSD1306_WHITE);
  }
  yield(); // Yield after humid/rice overlays

  // 4. Overlay CO2 indicators
  if (eco2 >= CO2_V_HIGH_THRESHOLD) {
    display.drawBitmap(0, 0, co2_v_high, 128, 64, SSD1306_WHITE);
  } else if (eco2 >= CO2_HIGH_THRESHOLD) {
    display.drawBitmap(0, 0, co2_high, 128, 64, SSD1306_WHITE);
  }
  yield(); // Yield after CO2 overlays

  // 5. Overlay VOC indicators
  if (tvoc >= VOC_HIGH_THRESHOLD) {
    display.drawBitmap(0, 0, voc_high, 128, 64, SSD1306_WHITE);
  } else if (tvoc >= VOC_MED_THRESHOLD) {
    display.drawBitmap(0, 0, voc_med, 128, 64, SSD1306_WHITE);
  } else {
    display.drawBitmap(0, 0, voc_low, 128, 64, SSD1306_WHITE);
  }
  yield(); // Yield after VOC overlays

  display.display();
  yield(); // Final yield after display
}

void showStatusBar(StatusBarSubMode mode) {
  yield(); // Yield before clearDisplay
  display.clearDisplay();
  yield(); // Yield after clear

  sensors_event_t humidityEvent, tempEventAHT;
  aht.getEvent(&humidityEvent, &tempEventAHT);
  float value = 0.0;
  const char* label = "";
  yield(); // Yield after AHT read

  // Common bar parameters
  const int BAR_Y = 20;
  const int BAR_HEIGHT = 14;
  const int BAR_START_X = 38;
  const int BAR_END_X = 91;

  display.setTextSize(1);
  display.setTextColor(SSD1306_WHITE);

  switch (mode) {
    case STATUS_TEMP:
      display.drawBitmap(0, 0, status_bar_temp, 128, 64, SSD1306_WHITE);
      value = tempEventAHT.temperature;
      label = "AHT Temp: ";
      // Clamp and map temperature: 25°C to 35°C
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, 25.0, 35.0), 25, 35, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 1);
      display.print(F(" C"));
      break;
    case STATUS_HUMID:
      display.drawBitmap(0, 0, status_bar_humid, 128, 64, SSD1306_WHITE);
      value = humidityEvent.relative_humidity;
      label = "AHT Humid: ";
      // Clamp and map humidity: 70% to 100%
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, 70.0, 100.0), 70, 100, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 1);
      display.print(F(" %"));
      break;
    case STATUS_PROBE_TEMP:
      display.drawBitmap(0, 0, status_bar_probe, 128, 64, SSD1306_WHITE);
      tempSensorDS18B20.requestTemperatures();
      value = tempSensorDS18B20.getTempCByIndex(0);
      label = "Probe Temp: ";
      // Clamp and map temperature: 25°C to 35°C
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, 25.0, 35.0), 25, 35, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 1);
      display.print(F(" C"));
      break;
    case STATUS_MOISTURE:
      display.drawBitmap(0, 0, status_bar_moist, 128, 64, SSD1306_WHITE);
      value = analogRead(MOISTURE_PIN);
      label = "Moisture: ";
      // Clamp and map moisture: 1200 (min) to 2200 (max)
      // display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, (float)MOISTURE_MIN, (float)MOISTURE_MAX), MOISTURE_MIN, MOISTURE_MAX, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      // NEW: Map raw value (0=wet, 4095=dry) to bar fill (full=wet, empty=dry)
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, (float)MOISTURE_RAW_WETTEST, (float)MOISTURE_RAW_DRYEST), MOISTURE_RAW_WETTEST, MOISTURE_RAW_DRYEST, BAR_END_X - BAR_START_X + 1, 0), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 0);
      break;
    case STATUS_CO2:
      display.drawBitmap(0, 0, status_bar_co2, 128, 64, SSD1306_WHITE);
      value = airQualitySensor.getECO2();
      label = "eCO2: ";
      // Clamp and map CO2: 400 to CO2_MAX_GRAPH
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, 400.0, (float)CO2_MAX_GRAPH), 400, CO2_MAX_GRAPH, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 0);
      display.print(F(" ppm"));
      break;
    case STATUS_VOC:
      display.drawBitmap(0, 0, status_bar_voc, 128, 64, SSD1306_WHITE);
      value = airQualitySensor.getTVOC();
      label = "TVOC: ";
      // Clamp and map VOC: 0 to VOC_MAX_GRAPH
      display.fillRect(BAR_START_X, BAR_Y, map(constrain(value, 0.0, (float)VOC_MAX_GRAPH), 0, VOC_MAX_GRAPH, 0, BAR_END_X - BAR_START_X + 1), BAR_HEIGHT, SSD1306_WHITE);
      display.setCursor(0, 0);
      display.print(label);
      display.print(value, 0);
      display.print(F(" ppb"));
      break;
    case STATUS_NONE:
      // This case is handled by showStatusFace, but good to have for completeness
      break;
  }
  display.display();
  yield(); // Final yield after display
}

// Function to map a float value from one range to another (utility for graph plotting)
long mapFloat(float x, float in_min, float in_max, long out_min, long out_max) {
  return (x - in_min) * (out_max - out_min) / (in_max - in_min) + out_min;
}