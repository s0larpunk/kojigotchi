unsigned long lastUserInteractionTime = 0;
bool newSensorDataAvailable = false; // Flag for screen refresh in graph mode

int graphWindowIndex = 0;   // Which 'window' of 256 samples is being displayed for graph
bool autoFollowGraph = true; // Automatically follow the latest data in graph mode
